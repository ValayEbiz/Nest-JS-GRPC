{
  "name": "@nest-grpc/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "generate-proto-types": "npx protoc --ts_proto_out=./types/ ./proto/*.proto --ts_proto_opt=nestJs=true"
  },
  "nx": {
    "targets": {
      "generate-proto-types": {
        "cache": true,
        "inputs": [
          "{workspaceRoot}/proto/*.proto"
        ]
      }
    }
  },
  "private": true,
  "dependencies": {
    "@grpc/grpc-js": "^1.14.1",
    "@grpc/proto-loader": "^0.8.0",
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/microservices": "^11.1.9",
    "@nestjs/platform-express": "^11.0.0",
    "axios": "^1.6.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "ts-proto": "^1.45.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@nx/eslint": "22.0.4",
    "@nx/eslint-plugin": "22.0.4",
    "@nx/js": "22.0.4",
    "@nx/nest": "22.0.4",
    "@nx/node": "22.0.4",
    "@nx/web": "22.0.4",
    "@nx/webpack": "22.0.4",
    "@nx/workspace": "22.0.4",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/node": "20.19.9",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "nx": "22.0.4",
    "prettier": "^2.6.2",
    "tslib": "^2.3.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.40.0",
    "webpack-cli": "^5.1.4"
  }
}